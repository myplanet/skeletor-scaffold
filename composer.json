{
  "name": "drupal/skeletor_scaffold",
  "type": "drupal-module",
  "description": "Skeletor Scaffolding",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/skeletor_scaffold",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/skeletor_scaffold",
    "source": "http://cgit.drupalcode.org/skeletor_scaffold"
  },
  "require": {
    "composer/installers": "^1.2",
    "oomphinc/composer-installers-extender": "dev-master",
    "webflo/drupal-finder": "^1.0",
    "myplanet/react-redux-skeletor": "*"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "myplanet/react-redux-skeletor",
        "version": "1.0",
        "type": "npm-package",
        "source": {
          "type": "git",
          "reference": "presentation",
          "url": "git@github.com:myplanet/react-redux-skeletor.git"
        },
        "require": {
          "composer/installers": "^1.2",
          "oomphinc/composer-installers-extender": "dev-master"
        }
      }
    }
 ],
 "autoload": {
    "classmap": ["scripts/skeletor/NpmPackage.php"]
  },
 "scripts": {
   "npm-install": "DrupalSkeletor\\NpmPackage::npmInstall",
   "npm-build": "DrupalSkeletor\\NpmPackage::npmBuild",
   "npm-test": "DrupalSkeletor\\NpmPackage::npmTest",
   "post-install-cmd": [
      "@composer npm-install",
      "@composer npm-build"
   ],
   "post-update-cmd": [
      "@composer npm-install",
      "@composer npm-build"
   ]
 },

  "extra": {
    "installer-paths": {
       "npm-package/{$name}": ["type:npm-package"]
    },
    "installer-types": ["npm-package"]
  }
}
